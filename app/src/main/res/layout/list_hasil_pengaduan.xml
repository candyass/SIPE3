<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:cardElevation="8dp"
        app:cardBackgroundColor="#F1F0F0"
        app:cardCornerRadius="16dp">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:textStyle="bold"
                android:textColor="#1E1F1E"
                android:drawableRight="@drawable/ic_arrow_24dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_no_kandidat" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>


        <de.hdodenhof.circleimageview.CircleImageView android:layout_width="40dp" android:layout_height="40dp"
                                                      android:src="@drawable/orang_1"
                                                      android:layout_marginTop="24dp"
                                                      app:layout_constraintTop_toBottomOf="@+id/list_hasil_pengaduan_no_kandidat"
                                                      app:layout_constraintStart_toStartOf="parent"
                                                      android:layout_marginStart="8dp"
                                                      android:id="@+id/circleImageView4"/>
        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_nama_text" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintStart_toEndOf="@+id/circleImageView4" android:layout_marginStart="8dp"
                app:layout_constraintTop_toTopOf="@+id/circleImageView4"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_daerah_1" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toEndOf="@+id/circleImageView4" android:layout_marginStart="8dp"
                app:layout_constraintTop_toBottomOf="@+id/list_hasil_pengaduan_nama_text"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

        <de.hdodenhof.circleimageview.CircleImageView android:layout_width="40dp" android:layout_height="40dp"
                                                      android:src="@drawable/orang_2"
                                                      android:layout_marginTop="16dp"
                                                      app:layout_constraintTop_toBottomOf="@+id/circleImageView4"
                                                      app:layout_constraintStart_toStartOf="@+id/circleImageView4"
                                                      android:id="@+id/circleImageView5"/>
        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_nama_wakil_text" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintStart_toEndOf="@+id/circleImageView5" android:layout_marginStart="8dp"
                app:layout_constraintTop_toTopOf="@+id/circleImageView5"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_daerah_2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toEndOf="@+id/circleImageView5" android:layout_marginStart="8dp"
                app:layout_constraintTop_toBottomOf="@+id/list_hasil_pengaduan_nama_wakil_text"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

        <de.hdodenhof.circleimageview.CircleImageView android:layout_width="40dp" android:layout_height="40dp"
                                                      android:src="@drawable/ic_pelanggaran_red_40dp"
                                                      android:layout_marginTop="16dp"
                                                      app:layout_constraintTop_toBottomOf="@+id/circleImageView5"
                                                      app:layout_constraintStart_toStartOf="@+id/circleImageView5"
                                                      android:id="@+id/circleImageView6"/>
        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_total_text" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toEndOf="@+id/circleImageView6" android:layout_marginStart="8dp"
                app:layout_constraintTop_toTopOf="@+id/circleImageView6"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
        <TextView
                android:text="TextView"
                android:layout_width="0dp"
                android:paddingBottom="8dp"
                android:layout_height="wrap_content"
                android:id="@+id/list_hasil_pengaduan_status_text"
                app:layout_constraintTop_toBottomOf="@+id/list_hasil_pengaduan_total_text" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/circleImageView6"
                android:layout_marginStart="8dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>